{
  "name": "sxBlock",
  "version": "1.6",
  "manifest_version": 2,
  "description": "Chrome extension to remove chafing SXSW tweets from your timeline.",
  "icons": {
  	"128":"block_128.png",
  	"48":"block_48.png",
	"16":"block_16.png"
  },
  "background": {
	"scripts": ["background.js"]
  },
  "permissions": [
    "tabs", 
    "http://twitter.com/*",
    "https://twitter.com/*"
  ],
  "content_scripts": [{
  	"matches": ["http://twitter.com/*", "https://twitter.com/*"],
    "js": ["jquery-1.9.1.min.js", "script.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": {
      "19": "block_19.png",
      "38": "block_38.png"
    },
    "default_title": "sxBlock"
  }
}